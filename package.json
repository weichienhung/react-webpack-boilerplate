{
  "name": "fillme",
  "version": "1.0.0",
  "description": "simple react-webpack boilerplate",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --config webpack.prod.config.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot --host 0.0.0.0 --port 8080 --inline --devtool eval --progress --colors",
    "start": "http-server ./"
  },
  "author": "shung",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.7.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "http-server": "^0.9.0",
    "image-webpack-loader": "^1.8.0",
    "moment": "^2.18.1",
    "node-sass": "^3.4.2",
    "normalize.css": "^4.1.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "webpack": "^1.12.14"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-eslint": "^6.0.0",
    "babel-jest": "^12.0.2",
    "babel-polyfill": "^6.8.0",
    "babel-preset-stage-2": "^6.5.0",
    "cross-env": "^1.0.7",
    "eslint": "2.5.3",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.4.0",
    "eslint-plugin-react": "^4.2.3",
    "react-hot-loader": "1.3.0",
    "run-sequence": "1.2.1",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.0",
    "webpack-dev-server": "^1.14.1"
  }
}
